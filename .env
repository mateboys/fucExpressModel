# 服务器配置
PORT=3000
NODE_ENV=development

# 数据库配置
DB_HOST=localhost
DB_PORT=3306
DB_USER=root          # 你的 MySQL 用户名
DB_PASSWORD=123456    # 你的 MySQL 密码
DB_NAME=express_demo  # 你要使用的数据库名
DB_CONNECTION_LIMIT=10

# JWT配置
JWT_SECRET=your-super-secret-jwt-token-key  # 修改为你的密钥
JWT_EXPIRES_IN=24h

# CORS配置
CORS_ORIGIN=http://localhost:5173  # 前端应用的地址

# 日志配置
LOG_LEVEL=debug

# API配置
API_PREFIX=/api
API_VERSION=v1